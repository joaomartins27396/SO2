<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>VoosServlet</servlet-name>
        <servlet-class>Servlet.VoosServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VoosServlet</servlet-name>
        <url-pattern>/consulta</url-pattern>
        <url-pattern>/compra</url-pattern>
        <url-pattern>/pesquisa</url-pattern>
        <url-pattern>/consultaResultado</url-pattern>
        <url-pattern>/compraEfetuada</url-pattern>
        <url-pattern>/pesquisaEfetuada</url-pattern>
        <url-pattern>/lista</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
